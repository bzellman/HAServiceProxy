{
  "name": "HA Service Proxy",
  "version": "1.0.0",
  "slug": "haserviceproxy",
  "description": "Securely access internal services via Home Assistant",
  "startup": "services",
  "boot": "auto",
  "options": {
    "services": [
      {
        "name": "Service 1",
        "ip": "192.168.1.2"
      }
    ],
    "sidebar_services": ["Service 1"],
    "ssl_cert": "/ssl/fullchain.pem",
    "ssl_key": "/ssl/privkey.pem",
    "log_level": "info"
  },
  "schema": {
    "services": [
      {
        "name": "str",
        "ip": "str"
      }
    ],
    "sidebar_services": ["str"],
    "ssl_cert": "str",
    "ssl_key": "str",
    "log_level": "str"
  },
  "arch": ["armv7", "aarch64", "amd64", "i386"],
  "icon": "mdi:server-network"
}
